{
  "rewrites": [
    { "source": "/components/(.*).js", "destination": "/components/$1.js" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/components/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
  